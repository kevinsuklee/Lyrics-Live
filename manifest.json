{
  "manifest_version": 2,

  "name": "Lyrics Live",
  "description": "This extension provides real-time lyrics while watching YouTube videos.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "rap-genius19px.png",
    "default_title": "Lyrics Live",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://secure.flickr.com/"
  ]
}
